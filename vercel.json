{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "./routes/*.js": {
      "maxDuration": 30
    },
    "./controllers/*.js": {
      "maxDuration": 30
    }
  }
}
